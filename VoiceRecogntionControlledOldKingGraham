///////////// DRAW VOICE TEXT BOX ///////////////////
function drawVoiceTextBox(){


T = final_transcript;
T = T.toLowerCase();

if (oldT != T){

//if(myaudio.paused){
textParser(T);
oldT = T;
//}

}

var newlinerunningcounter = 0;

var oldnewlinespace = 0;
var recentspace = 0;

var newlinespace = final_transcript.indexOf(" ");

newlinerunningcounter = newlinespace - oldnewlinespace;

//**makesurenewlinespace isn't empty


for (i = 0; i < 25; i++) {

var newlinespace = final_transcript.indexOf(" ", (recentspace+1));
recentspace = newlinespace;

newlinerunningcounter = newlinespace - oldnewlinespace;

}



//text on screen


//
textTimestamp = new Date().getTime();

if (final_transcript != ""){
startTextTime = textTimestamp;
displayVar = final_transcript;

if (displayVar[0] == " "){
displayVar = displayVar[1].toUpperCase() + displayVar.substring(2);
} else{
displayVar = displayVar[0].toUpperCase() + displayVar.substring(1);
}

}

if(displayVar != ""){
if ((textTimestamp - startTextTime) < 2000){




//////
if (displayVar.length > 0 && lookingatmap == 0){

var lineheight = 10;
var lines = displayVar.split('\n');




 ctx.fillStyle = "#000000";
  ctx.fillRect(0, 280, 546, 20);
  
  
  ///102 = 469
  //444 = 811
//large square





//FONT settings
ctx.fillStyle = "white";
ctx.textAlign = "center";
ctx.font = "10px Garamond bold";


//DRAW THE WORDS
for (var i = 0; i<lines.length; i++){
  
    ctx.fillText(lines[i], 273, 290 );
}
}

} else {
oldT = "";

}
}

/////////////////////
//reset final transcript

final_transcript = "";
transcript = "";

}




///////////////
/////////////////////////////////////////////////////////////////////////////
///////////////////////////END VOICE2////////////////////////////////////////////
///////////////////////////////////////////////////////////////////////////

</script>


<script>

function updateGameArea() {
  if (myGamePiece.crashWith(myObstacle)) {
    speed = 0;
    yspeed = 0;
    myGamePiece.speedX = 0;
    myGamePiece.speedY = 0;
  } 
 
    myGameArea.clear();
    
    oldBackground.update();
    oldForeground.update();
     myBackground.newPos();  
     
    myBackground.update();
    
    
    
    myGamePiece.speedX = speed;
    myGamePiece.speedY = yspeed; 
    
    
    
    if (moveVar == ""){
    toggle = 1;
    
    }
    
   
    if (moveVar != "" && textVar2.length == 0){
    
    
    
    //PRESSED LEFT
    if (moveVar == "left") {
    
    
    
    toggle = 0;
    yspeed = 0;
    
        
        myGamePiece.speedX = -1; 
        speed = -1;
        if(walking == 1){
        myGamePiece.image.src = walkleft[walkiter];
        }
        
        
        }
        //PRESSED RIGHT
    if (moveVar == "right") {
    
    
    
   
    toggle = 0;
    yspeed = 0;
   
     
        myGamePiece.speedX = 1; 
        speed = 1; 
         if(walking == 1){
        myGamePiece.image.src = walkright[walkiter];
        }
        }
        
        
        //PRESSED UP
    if (moveVar == "up") {
    
   
    
    toggle = 0;
    speed = 0;
       
        myGamePiece.speedY = -1; 
        yspeed = -1;
       //  if(walking == 1){
        //myGamePiece.image.src = walkback[3];
        
        //}
    
    }
    //PRESSED DOWN
    if (moveVar == "down") {
    
    
    
     toggle = 0;
     speed = 0;
    
        myGamePiece.speedY = 1; 
        yspeed = 1; 
        
        

    
    }
    
      if (moveVar == "stop") {
      obstacleAvoidMode = 0;
      originalDirection = moveVar;
      moveVar = "";
      if(walking == 1){
        if(speed == -1){
            myGamePiece.image.src = walkleft[2];
            } else if(speed == 1){
            myGamePiece.image.src = walkright[2];
            } 
        }
      
      myGamePiece.speedY = 0; 
        yspeed = 0;
        myGamePiece.speedX = 0; 
        speed = 0;
         toggle = 0;
    
      }
    originalDirection = moveVar;
    //moveVar = "";
 
    }
    
   
   
    myGamePiece.newPos();    
    myGamePiece.update();
    
    myForeground.update();
    myForegroundItem.update();
    myForegroundItem2.update();
    
    if(destinationX != -1 && destinationY != -1){
    wayfind();
    }
    
  
    
    if(lookingatmap == 0){
   ctx.fillStyle = "#000000";
  ctx.fillRect(0, 280, 546, 20);
  }
 
   
   
    drawTextBox(textVar);
  


    drawVoiceTextBox();
  
    
  
    

    
    ///////////////////////////////////////////////////////////////
///////////////////////////VOICE////////////////////////////////////////////
///////////////////////////////////////////////////////////////////////////

spoken.listen.on.end(continueCapture);
spoken.listen.on.error(continueCapture);

 


  
/////////////////////////////////////////////////////////////////////////////
///////////////////////////END VOICE////////////////////////////////////////////
///////////////////////////////////////////////////////////////////////////  
    
    
    
    
  
    
} //end update gamearea
