}//end backgroundmusic function



//////////////////////WAYFIND/////////////////////////////////////////////////
function wayfind(){


//coordinates are top left corner of graham image
var currentXcoordinate = myGamePiece.x;
var currentYcoordinate = myGamePiece.y;

var xdistance = destinationX - currentXcoordinate;
var ydistance = destinationY - currentYcoordinate;

//get the direction you're going
//calculate shortest distance around object (if going x get shortest y around)
//then go that direction until you reach that point and turn



//TOGGLE X AND Y IF STUCK STUCK STUCK STUCK STUCK STUCK STUCK
if (oldXcoordinate == currentXcoordinate && oldYcoordinate == currentYcoordinate){

obstacleAvoidMode = 1;

if(originalDirection == ""){
obstacleAvoidMode = 0;
}

//console.log("this here");
//console.log("originalDirection: " + originalDirection);
//console.log("wayfindcounter: " + wayfindcounter);

} //END STUCK


//RESET OBSTACLE AVOID IF NEW DESTINATION 
if(destinationX != oldDestinationX || destinationY != oldDestinationY){
obstacleAvoidMode = 0;
oldDestinationX = destinationX;
oldDestinationY = destinationY;

if(Math.abs(xdistance) > Math.abs(ydistance)){ //x distance larger
    wayfindcounter = 1;
}else if (Math.abs(xdistance) <= Math.abs(ydistance)){ //y distance larger
    wayfindcounter = 2;
}


}




//STARTING



if(oldXcoordinate == currentXcoordinate && oldYcoordinate == currentYcoordinate  && wayfindcounter == 0){

if(Math.abs(xdistance) > Math.abs(ydistance)){ //x distance larger
    wayfindcounter = 1;
}else if (Math.abs(xdistance) <= Math.abs(ydistance)){ //y distance larger
    wayfindcounter = 2;
}

obstacleAvoidMode = 0;

}


////////////// 000000000000000000000000000000000
 console.log("obstacleAvoidMode: " + obstacleAvoidMode);
if(obstacleAvoidMode == 0){

if (wayfindcounter == 1){ //X
    
    
    
    if(destinationX >= currentXcoordinate){ //destination right
   
    moveVar = "right";
    
    } else { //destination left
    
    moveVar = "left";
    }
     myGamePiece.speedY = 0; 
    yspeed = 0; 
    
    
    
    if(Math.abs(currentXcoordinate - destinationX) < 3){
    
    wayfindcounter = 2;
    }

} else if (wayfindcounter == 2){ //Y

    if(destinationY >= currentYcoordinate){ //destination down
    
    moveVar = "down";
    } else { //destination up
   
    moveVar = "up";
    }

    myGamePiece.speedX = 0; 
    xspeed = 0; 

    if(currentYcoordinate == destinationY){
    wayfindcounter = 1;
    }

}
////////////////    11111111111111111111111111111111
} else if(obstacleAvoidMode == 1){ //IDENTIFY WHICH WAY TO TURN



if(originalDirection == "right" || originalDirection == "left"){

if(theObstacleY < 5 && theObstacleY + theObstacleHeight > 250){ //SOLID VERTICAL Y BAR

obstacleAvoidMode = 4;

}else{

var distanceToObstacleTop = Math.abs(theObstacleY - currentYcoordinate);
var distanceToObstacleBottom = Math.abs((theObstacleY + theObstacleHeight) - currentYcoordinate);



if(theObstacleY < 5){ //IF OBSTACLE OFF THE SCREEN
moveVar = "down";
}else if(theObstacleY + theObstacleHeight > 250){
moveVar = "up";
}else if(destinationY > theObstacleY + theObstacleHeight){
moveVar = "down";
} else if(distanceToObstacleTop > distanceToObstacleBottom){
moveVar = "down";
} else if(theObstacleY < 20){
moveVar = "down";
} else{
moveVar = "up";
}


obstacleAvoidMode = 2;
}

} else if(originalDirection == "up" || originalDirection == "down"){



if(theObstacleX < 5 && theObstacleX + theObstacleWidth > 535){ //SOLID X BAR

obstacleAvoidMode = 3;

}else{

var distanceToObstacleLeft = Math.abs(theObstacleX - currentXcoordinate);
var distanceToObstacleRight = Math.abs((theObstacleX + theObstacleWidth) - currentXcoordinate);

if(theObstacleX < 5){ //IF OBSTACLE OFF THE SCREEN
moveVar = "right";
}else if(theObstacleX + theObstacleWidth > 535){
moveVar = "left";
} else if(destinationX > theObstacleX + theObstacleWidth){
moveVar = "right";

} else if(distanceToObstacleLeft > distanceToObstacleRight){
moveVar = "right";

} else{
moveVar = "left";
}


obstacleAvoidMode = 2;

}

}



/*
var theObstacleWidth;
var theObstacleHeight;
var theObstacleX; 
var theObstacleY;

*/
////////////////////// 22222222222222222222222222222222
}  else if(obstacleAvoidMode == 2){ //DECIDE AFTER CLEARING OBSTACLE

if(moveVar == "down"){
if(currentYcoordinate > (theObstacleHeight + theObstacleY)){
//obstacleAvoidMode = 0;

if(destinationY > currentYcoordinate){ //IF DESTINATION IS BELOW GRAHAM
moveVar = "down";
} else if (destinationY <= currentYcoordinate){
if(destinationX > currentXcoordinate){
moveVar = "right";
} else{
moveVar = "left";
}

}

} 

} else if(moveVar == "up"){


if(currentYcoordinate < theObstacleY){
//obstacleAvoidMode = 0;

if (destinationY >= currentYcoordinate){
if(destinationX > currentXcoordinate){
moveVar = "right";
} else{
moveVar = "left";
}

}

} 


}else if(moveVar == "left"){

if(currentXcoordinate < theObstacleX){
//obstacleAvoidMode = 0;

if (destinationX >= currentXcoordinate){
if(destinationY <= currentYcoordinate){
moveVar = "up";
} else{
moveVar = "down";
}

}

} 

}else if(moveVar == "right"){

if(currentXcoordinate > theObstacleX + theObstacleWidth){
//obstacleAvoidMode = 0;

if (destinationX <= currentXcoordinate){
if(destinationY <= currentYcoordinate){
moveVar = "up";
} else{
moveVar = "down";
}

}

} 

}


/// 333333333333333333333333333333333333
} 

if(obstacleAvoidMode == 3){ // solid X bar

if(destinationX > currentXcoordinate){ //destination right
   
    moveVar = "right";
    
    } else if(Math.abs(destinationX - currentXcoordinate) < 3){
    if(destinationY <= currentYcoordinate){
    moveVar = "up";
    } else{
    moveVar = "down";
    }
    
    }else if(destinationX <= currentXcoordinate){ //destination left
    
    moveVar = "left";
    }

}else if(obstacleAvoidMode == 4){ // solid Y bar

if(destinationY > currentYcoordinate){ //destination down
   
    moveVar = "down";
    
    } else if(Math.abs(destinationY - currentYcoordinate) < 3){ //at destination
    
    if(destinationX <= currentXcoordinate){
    moveVar = "left";
    } else if(destinationX > currentXcoordinate){
    moveVar = "right";
    }
    
    }else { //destination right
    
    moveVar = "right";
    }



} //END OBSTACLE AVOID MODE






oldXcoordinate = myGamePiece.x;
oldYcoordinate = myGamePiece.y;

//AT DESTINATION
if ( Math.abs(destinationX - currentXcoordinate) < 3 && Math.abs(destinationY - currentYcoordinate) < 3){
if(finalimage != ""){
myGamePiece.image.src = finalimage;

if(room == "throneRoom1"){


if(Math.abs(myGamePiece.y - 88) < 8){

oldthronefile = youngleft[walkiter];
OldBackgroundfile = youngleft[walkiter];
oldBackground = new component(546, 280, OldBackgroundfile,(myGamePiece.x-326), -53, "image");
speed = -1;
}


}//end throneroom1

}
wayfindcounter = 0;
destinationX = -1;
destinationY = -1;
moveVar = "stop";
}



} // end wayfind function

///////////////////////////////////////////////////////////////////////////////
///////////////////NEW SCREEN ////////////////////////////////////////////////
function changeToNewScreen(backgroundFile, foregroundFile, position, startPositionX,startPositionY){



backgroundmusic();

if(textVarReset == 0){
textVar = "";
textVar2 = "";
textVar3 = "";
} else{
textVarReset = 0;
}

destinationX = -1; //reset wayfind
destinationY = -1;
obstacleAvoidMode = 0;
wayfindcounter = 0;

oldBackground = new component(546, 280, OldBackgroundfile, 0, 0, "image");
oldForeground = new component(546, 280, OldForegroundfile, 0, 0, "image");
//oldBackground.update();
//oldForeground.update();

myBackground = new component(546, 280, backgroundFile, 0, 0, "image");
myForeground = new component(546, 280, foregroundFile, 0, 0, "image");


OldBackgroundfile = backgroundFile;

OldForegroundfile = foregroundFile;


foregroundItems();

createObstacles();
myGamePiece.x = startPositionX;
myGamePiece.y = startPositionY;

if (position == "goingUp"){
yspeed = -1;
}
else if (position == "goingDown"){
yspeed = 1;
}
else if (position == "goingRight"){
speed = 1;
}
else if (position == "goingLeft"){
speed = -1;
}



}
////////////NEW SCREEN NO BACKGROUND BACKGROUND MUSIC UPDATE ///////////
function changeToNewScreenNoMusic(backgroundFile, foregroundFile, position, startPositionX,startPositionY){




if(textVarReset == 0){
textVar = "";
textVar2 = "";
textVar3 = "";
} else{
textVarReset = 0;
}

destinationX = -1; //reset wayfind
destinationY = -1;
obstacleAvoidMode = 0;
wayfindcounter = 0;

oldBackground = new component(546, 280, OldBackgroundfile, 0, 0, "image");
oldForeground = new component(546, 280, OldForegroundfile, 0, 0, "image");
//oldBackground.update();
//oldForeground.update();

myBackground = new component(546, 280, backgroundFile, 0, 0, "image");
myForeground = new component(546, 280, foregroundFile, 0, 0, "image");


OldBackgroundfile = backgroundFile;

OldForegroundfile = foregroundFile;


foregroundItems();

createObstacles();
myGamePiece.x = startPositionX;
myGamePiece.y = startPositionY;

if (position == "goingUp"){
yspeed = -1;
}
else if (position == "goingDown"){
yspeed = 1;
}
else if (position == "goingRight"){
speed = 1;
}
else if (position == "goingLeft"){
speed = -1;
}



}
///////////////////////////////////////////////////////
// PLAY AUDIO FUNCTION

function playaudio(audio){

//if(audio != oldaudio && myaudio.paused){
if(audio != oldaudio){
myaudio.pause();
oldaudio = audio;
 myaudio = new Audio(audio);
        myaudio.play();
        
}

}


////////////SPECIAL COLLISION EVENTS///////////////////////////////////////////
function checkSpecialCollisionEvents(){

if (room == "throneRoom1"){
    if (myGamePiece.x > 540){
        if (CompletedThroneRoomRequirements == 0){
        
      
       
           playaudio("gamesounds/gvalanicelove.mp3");
        
       
       
     
        
        textVar = "I just can't leave without \ntelling Valanice I love her";
        textVar2 = "";
        textVar3 = "";
        
        } else if (CompletedThroneRoomRequirements == 1){
        room = "throneRoom2";
        oldBackground = new component(546, 280, "throne2.png",0, 0, "image"); //mirrorfront
        myBackground.image.src = "throne2.png";
        changeToNewScreen("throne2.png", "throne2foreground.png", "goingRight",80,153);
        }
        
    }
    
     
    
} else if (room == "throneRoom2"){
    if (myGamePiece.y > 212){
    room = "throneRoom3";
    
    if(CompletedCageRequirements == 0){
    changeToNewScreen("throne3cage.png", "throne3foreground.png", "goingDown",270,100);
    }else{
     changeToNewScreen("throne3.png", "throne3foreground.png", "goingDown",270,100);
    }
    
    }
    else if (myGamePiece.x < 20){
    
    room = "throneRoom1";
    changeToNewScreen("throneroom.png", "throneroomforeground.png", "goingLeft",530,153);
    
    }
}
else if (room == "throneRoom3"){

if (CompletedCageRequirements == 0 && CompletedCageKey == 0){

if(textVar == "" && myaudio.paused){
cedArray = ["Cedric the Grey: \n Going out for another adventure?\nJust like the old days!","Cedric the Grey: \nOld friend!\nWhat's that over there?\nLook!", "Cedric the Grey: \nLook!\n'Look!'\nLook"];
textVar = cedArray[textIter];
if(textIter == 0){
textVar2 = "Graham: \nThat's right,\nbut not exactly";
}

if (myGamePiece.y > 123){
myGamePiece.y -= 1;
} else if (myGamePiece.y > 123){
myGamePiece.y += 1;
}

destinationX = -1; //reset wayfind
destinationY = -1;
obstacleAvoidMode = 0;
moveVar ="stop";


if(textIter == 0){
playaudio("gamesounds/canotheradventure.mp3");
} else if(textIter == 1){
playaudio("gamesounds/coldfriend.mp3");
} else if(textIter == 2){
playaudio("gamesounds/clooklook.mp3");
}

textIter = textIter + 1;
if (textIter > 2){ 
textIter = 0;
}
}

//textVar = "Cedric the Grey: \n Down here Graham!";

}
else if (CompletedCageKey == 1){
playaudio("gamesounds/cseekey.mp3");
textVar = "Cedric the Grey:\nI see the key!";
textVar2 = "";
textVar3 = "";
} else if(CompletedCageKey == 2 && CompletedCageRequirements == 0){
playaudio("gamesounds/cmyhero.mp3");
textVar = "Cedric the Grey:\nMy hero";
textVar2 = "";
textVar3 = "";
}else if (CompletedCageRequirements == 1){
    if (myGamePiece.y > 212){
    room = "castleFront";
    myBackground.image = new Image();
     myBackground.image.src = "castleFront.png";
     myBackground.image = new Image();
     myBackground.image.src = "castleFrontflag2.png";
     myBackground.image = new Image();
     myBackground.image.src = "castleFrontflag3.png";
     myBackground.image = new Image();
     myBackground.image.src = "castleFrontflag4.png";
    changeToNewScreen("castleFrontflag4.png", "transparent.png", "goingDown",379,143);
    }
    else if (myGamePiece.y < 120){
    room = "throneRoom2";
    changeToNewScreen("throne2.png", "throne2foreground.png", "goingUp",270,200);
    }
}

} //end throneroom3
else if (room == "castleFront"){
 if (myGamePiece.y > 189){
 playaudio("gamesounds/owlhoot.mp3");
    textVar = "Cedric the Grey:\nNoooooooo Graham!\n Mermaids, pull him \nout of the moat!";
    textVar2 = "";
textVar3 = "";
    myGamePiece.y = 180
    destinationX = -1; //reset wayfind
destinationY = -1;
obstacleAvoidMode = 0;
moveVar ="stop";
    }
    else if (myGamePiece.x > 540){
    
     if (CompletedCastleFrontRequirements == 0){
     playaudio("gamesounds/mermaidohgrammy.mp3");
     textVar = "Mermaid:\nOh, Grammy!";
     textVar2 = "";
textVar3 = "";
     myGamePiece.x -= 1;
    destinationX = -1; //reset wayfind
    destinationY = -1;
    obstacleAvoidMode = 0;
    moveVar ="stop";

     }else{
    room = "castleFront2";
    changeToNewScreen("castleFront2.png", "transparent.png", "goingRight",15,180);
    
    
    if (gaveLoadOwlInfo == 0){
    speed = 0;
    destinationX = -1;
    destinationY = -1;
    moveVar = "stop";
    playaudio("gamesounds/owlhoot.mp3");
    textVar = "Cedric the Grey:\nYou've made beautiful progress.\nWhenever you wish to return \nto this point in time, \nlook in the magical mirror \nand speak the incantation,\n'Load Owl'";
    textVar2 = "";
textVar3 = "";
    gaveLoadOwlInfo = 1;
    }
    }
    }
}//end castleFront

else if (room == "castleFront2"){
if (myGamePiece.x < 7){
 room = "castleFront";
 
   myBackground.image = new Image();
     myBackground.image.src = "castleFront.png";
     myBackground.image = new Image();
     myBackground.image.src = "castleFrontflag2.png";
     myBackground.image = new Image();
     myBackground.image.src = "castleFrontflag3.png";
     myBackground.image = new Image();
     myBackground.image.src = "castleFrontflag4.png";
 
changeToNewScreen("castleFrontflag4.png", "transparent.png", "goingLeft",530,180);
}
    else if (myGamePiece.y > 189){
    playaudio("gamesounds/owlhoot.mp3");
    textVar = "Cedric the Grey:\nNoooooooo!\n Mermaids, pull King Graham \nout of the moat!";
    textVar2 = "";
textVar3 = "";
    myGamePiece.y = 180;
    destinationX = -1; //reset wayfind
destinationY = -1;
moveVar = "stop";
obstacleAvoidMode = 0;
    }
    else if (myGamePiece.x > 375 && myGamePiece.x < 454 && myGamePiece.y > 169){
    playaudio("gamesounds/owlhoot4.mp3");
    textVar = "Cedric the Grey:\nCareful on that old bridge!\n Mermaids, pull King Graham \nout of the moat!";
    textVar2 = "";
textVar3 = "";
    if(moveVar == "down" || moveVar == "left"){
        myGamePiece.y = 180;
        myGamePiece.x = 350;
    } else{
    myGamePiece.y = 164;
    myGamePiece.x = 388;
    }
        destinationX = -1;
        destinationY = -1;
        moveVar = "stop";
        
    }
    else if (myGamePiece.x > 290 && myGamePiece.x < 318 && myGamePiece.y < 163 && myGamePiece.y > 130){
    playaudio("gamesounds/owlhoot4.mp3");
    textVar = "Cedric the Grey:\nCareful on that old bridge!\n Mermaids, pull King Graham \nout of the moat!";
    textVar2 = "";
textVar3 = "";
if(moveVar == "down" || moveVar == "left"){
        myGamePiece.y = 180;
        myGamePiece.x = 350;
    } else{
     myGamePiece.y = 157;
    myGamePiece.x = 388;
        }
    }
    else if (myGamePiece.y < 127){
    if(plantedSeeds == 0){
    room = "garden";
    changeToNewScreen("garden.png", "transparent.png", "goingUp",432,215);
    }else{
    room = "gardenStalk";
    changeToNewScreen("gardenStalk.png", "transparent.png", "goingUp",432,215);
    }
    
}
    else if (myGamePiece.x > 540){
    myBackground.image = new Image();
     myBackground.image.src = "C1pooltoy2.png";
    myBackground.image = new Image();
     myBackground.image.src = "C1pooltoy.png";
    
    room = "C1pooltoy";
    changeToNewScreen("C1pooltoy.png", "transparent.png", "goingRight",15,180);
}

} //end castleFront2


else if (room == "garden"){
    if (myGamePiece.y < 34){
    room = "gardenNorthCliff";
    changeToNewScreen("gardenNorthCliff.png", "transparent.png", "goingUp",320,215);
     myBackground.image = new Image();
     myBackground.image.src = "gardenNorthCliffForeground1.png";
     myBackground.image = new Image();
     myBackground.image.src = "gardenNorthCliffForeground2.png";
     myBackground.image = new Image();
     myBackground.image.src = "gardenNorthCliffForeground3.png";
     myBackground.image = new Image();
     myBackground.image.src = "gardenNorthCliffForeground4.png";
    }
    else if (myGamePiece.y > 214){
    
     myBackground.image = new Image();
     myBackground.image.src = "castleFront24.png";
   myBackground.image = new Image();
     myBackground.image.src = "castleFront23.png";
    myBackground.image = new Image();
     myBackground.image.src = "castleFront22.png";
     myBackground.image = new Image();
     myBackground.image.src = "castleFront2.png";
    
     myForegroundItem.image = new Image();
     myForegroundItem.image.src = "castleFront2Foreground4.png";
     myForegroundItem.image = new Image();
     myForegroundItem.image.src = "castleFront2Foreground3.png";
     myForegroundItem.image = new Image();
     myForegroundItem.image.src = "castleFront2Foreground2.png";
     myForegroundItem.image = new Image();
     myForegroundItem.image.src = "castleFront2Foreground1.png";
    
    
    room = "castleFront2";
