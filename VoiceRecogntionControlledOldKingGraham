} else if (iterFour == 3){
myBackground = new component(546, 280, "gardenNorthCliffForeground4.png",0, 0, "image");
}
iterFour = iterFour + 1;
if (iterFour > 3){
iterFour = 0;
}
}//end gardenNorthCliff

else if (room == "throneRoom1" && bowing == 1){

destinationX = -1;
destinationY = -1;
moveVar = "stop";
speed = 0;
yspeed = 0;

if (iterFour == 0){

myGamePiece = new component(52, 64, "bow1.png", 209, myGamePiece.y, "image");
oldthronefile = "mirrorbow1.png";

} else if (iterFour == 1 || iterFour == 2){
