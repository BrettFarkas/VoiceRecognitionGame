destinationX = 257;
destinationY = 89;
}else if (T.includes("tomato")|| T.includes("rotten") ) {
destinationX = 191;
destinationY = 175;
}else if (T.includes("baby")|| T.includes("tear") ) {
destinationX = 131;
destinationY = 193;
}else if (T.includes("cling")|| T.includes("vine") ) {
destinationX = 112;
destinationY = 89;
}else if (T.includes("sour")|| T.includes("grape") ) {
destinationX = 112;
destinationY = 89;
}else if (T.includes("wall")|| T.includes("flower") || T.includes("flour")) {
destinationX = 368;
destinationY = 98;
}else if (T.includes("snap")|| T.includes("dragon") ) {
destinationX = 365;
destinationY = 157;
}else if (T.includes("berg")|| T.includes("lettuce") ) {
destinationX = 273;
destinationY = 145;
}

}else if(room == "greenisles2"){
  if (T.includes("up")||T.includes("north")){
moveVar = "up";
}else if (T.includes("down")||T.includes("south")) {
moveVar ="down";
}else if (T.includes("left")||T.includes("west")) {
moveVar ="left";
}else if (T.includes("right")||T.includes("east")) {
moveVar ="right";
}else if (T.includes("stop")) {
moveVar ="stop";
destinationX = -1;
destinationY = -1;
}else if (T.includes("door") || T.includes("leave")|| T.includes("exit")|| T.includes("basement") ) {
destinationX = 61;
destinationY = 147;
}else if (T.includes("bell")|| T.includes("pepper") ) {
destinationX = 242;
destinationY = 130;
}else if (T.includes("dandy")|| T.includes("lion") ) {
destinationX = 347;
destinationY = 183;
}else if (T.includes("ear")|| T.includes("corn") ) {
destinationX = 179;
destinationY = 118;
}else if (T.includes("lady")|| T.includes("bug") ) {
destinationX = 179;
destinationY = 153;
}else if (T.includes("potato")|| T.includes("eye") ) {
destinationX = 368;
destinationY = 130;
}

}




//////////////
} //end textentryfunction

