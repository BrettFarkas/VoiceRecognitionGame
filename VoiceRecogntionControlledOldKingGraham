function textEntryFunction() {



//console.log("textEntryFunction Activated");
//console.log("Final transcript: " + final_transcript);

var resultslength = event.results.length;
//console.log("resultslength is: " + resultslength);
//console.log("voice results is: " + event.results[resultslength - 1][0].transcript);

