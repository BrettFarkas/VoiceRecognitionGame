////////////////DRAW TEXT BOX//////////////////////////////////////
//to do: change px to pt and adjust box width accordingly?? or px is better

function drawTextBox(textVarinMethod){

textVar = textVarinMethod;

//textVar = "1234567890\n123456789012345"; //25 char per line



if (textVar.length > 0){

textNormalTimestamp = new Date().getTime();

if(textVar != oldtextvar){
oldtextvar = textVar;





textboxTimerDuration = textVar.length * 74;
if(textboxTimerDuration < 2000){
textboxTimerDuration = 2000;
}

if((textNormalTimestamp - startNormalTextTime) >= textboxTimerDuration){
startNormalTextTime = textNormalTimestamp;
}
}


// 2000 TIMER 2000 TIMER 2000 TIMER
if ((textNormalTimestamp - startNormalTextTime) < textboxTimerDuration + 500){

var lineheight = 25;
var lines = textVar.split('\n');

textHeight = (lines.length * 25) + (lines.length * 3) + 31;


textBoxTop = 140 - (textHeight/2);
textBoxBottom = 140 + (textHeight/2);
 ctx.fillStyle = "#FFFFFF";
  ctx.fillRect(98, textBoxTop, 350, textHeight);
  
  
//TO DO: CHANGE 98 AND 350 ABOVE (B0X) AND 102 AND 444 BELOW (RED LINE) WITH LONGEST LINE WIDTH + BUFFER SPACE - FIND THIS 273 = middle x of screen

ctx.beginPath();
ctx.moveTo(102, textBoxTop + 3);
ctx.lineTo(102, textBoxBottom - 3);
ctx.lineTo(444, textBoxBottom - 3);
ctx.lineTo(444, textBoxTop + 3);
ctx.lineTo(102, textBoxTop + 3);
ctx.strokeStyle = "darkred";
ctx.stroke();

ctx.fillStyle = "black";
ctx.textAlign = "center";
ctx.font = "25px Arial bold";
//ctx.fillText(textVar, 273, textBoxTop + 31);

//DRAW THE WORDS
for (var i = 0; i<lines.length; i++){
    ctx.fillText(lines[i], 273, textBoxTop + 31 + (i*lineheight) );
}

if (textboxTimerDuration - (textNormalTimestamp - startNormalTextTime) <  100){



textVar = "";

T = "";
final_transcript = "";


if (textVar == ""){
if (textVar2.length > 0){
textVar = textVar2;
textVar2 = "";
startNormalTextTime = textNormalTimestamp;
textboxTimerDuration = textVar.length * 74;

if(textboxTimerDuration < 2000){
textboxTimerDuration = 2000;
}

} else if (textVar3.length > 0){
textVar = textVar3;
textVar3 = "";
startNormalTextTime = textNormalTimestamp;
textboxTimerDuration = textVar.length * 74;
if(textboxTimerDuration < 2000){
textboxTimerDuration = 2000;
}
    } else{
    startNormalTextTime = 0;
    }
}


}

}//end 2000 timer



} //end textvar.length >0







} //end drawtextbox

/////////////////////////////////////////////////////////////////////
function textParser(T) {

   
   //var T = final_transcript;
   T = T.toLowerCase();
   //document.getElementById('textBox').value = "";
   
   

   
   if (room == "throneRoom1"){
   
   
   
   if (T.includes("i love you")){
   
        if(myGamePiece.x < 245){
        
         playaudio("gamesounds/val1.mp3");
        
        
        
    
    
    textVar = "Graham: I love you";
    textVar2 = "Valanice: My love for you is \n deeper than Neptune's kingdom,\n and my love reaches higher\n than a giant standing \namongst the clouds.\n  Go, Graham, with all of my love";
    
textVar3 = "";
        CompletedThroneRoomRequirements = 1;
        } else {
        textVar = "Graham isn't close enough\nto tell her that";
        textVar2 = "";
textVar3 = "";
        }
    }else if (T.includes("talk")||T.includes("hello") || T.includes("speak")){ 
    
    if ((T.includes("valanice")) || (T.includes("woman")) || (T.includes("queen")) || (T.includes("lady"))){
     textVar ="Is there something you \nwant to say Graham?";
     }
        else if ((T.includes("rosella")) || (T.includes("girl")) || (T.includes("princess")) || (T.includes("daughter"))){
        playaudio("gamesounds/lcareful.mp3");
     textVar ="Rosella: Are you sure you don't \nwant to rest\nin your comfortable bed? \nBe careful out there, father";
     }
