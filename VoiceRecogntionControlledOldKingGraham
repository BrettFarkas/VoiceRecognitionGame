<script>
/////////////////////////////////////////////////////////////////////////////
///////////////////////////VOICE////////////////////////////////////////////
///////////////////////////////////////////////////////////////////////////
var final_transcript = "";


function startCapture() { 
    spoken.listen({ continuous : true }).then( transcript =>
        console.log("Captured transcript: " + transcript)


    ).catch( e => true );
    
    console.log("startCaptureactivated");
   
}


async function continueCapture() {
    await spoken.delay(3);
    if (spoken.recognition.continuous) startCapture();
}

/*
function stopCapture() {
    spoken.recognition.continuous = false;
    spoken.listen.stop();
}
*/

async function runNow(){
let transcript = await spoken.listen()
spoken.listen.on.end(continueCapture);
spoken.listen.on.error(continueCapture);
startCapture();

//console.log("Text: " + transcript)

//console.log("runNowactivated")
 textEntryFunction();

 }

</script>

<script nomodule src="./spoken.js"></script>
<script type="module">
import spoken from './build/spoken.js';
( async e => {
'use strict';

// Setup Listener Events
spoken.listen.on.partial( transcript => { 
//console.log(transcript);

//console.log("partial Transcript activated");   
final_transcript = transcript;


//IDENTIFY SILENCE IN TEXT AND RESTART SENTENCE
//speakTimestamp = new Date().getTime();

